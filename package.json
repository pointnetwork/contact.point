{
  "name": "template.point",
  "version": "0.0.1",
  "author": "Point Network",
  "description": "Template example zapp - use as a scaffold to create more Zapps!",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@openzeppelin/contracts": "^4.4.2",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "clear": "rm -rf public/ .cache .parcel-cache && mkdir public",
    "watch:docker": "./watch.docker.js",
    "build": "npm run clear && cp src/*.html ./public/. && cp src/*.css ./public/. && parcel build src/index.tsx --dist-dir ./public --no-source-maps",
    "watch": "npm run clear && cp src/*.html ./public/. && cp src/*.css ./public/. && parcel watch src/index.tsx --dist-dir ./public/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.17",
    "@types/react": "^17.0.49",
    "@types/react-dom": "^18.0.6",
    "parcel": "^2.0.0-rc.0",
    "typescript": "^4.8.3"
  }
}
